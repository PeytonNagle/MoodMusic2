{
  "environment": "prod",
  "flask": {
    "debug": false
  },
  "database": {
    "connection_pool": {
      "min_connections": 5,
      "max_connections": 20
    },
    "save_queue": {
      "enabled": true,
      "max_size": 200,
      "behavior_on_full": "block"
    }
  },
  "request_handling": {
    "song_limits": {
      "min": 5,
      "max": 50
    }
  },
  "gemini": {
    "token_limits": {
      "recommendations_cap": 15000
    }
  }
}
