{
  "environment": "dev",
  "ai_provider": {
    "default": "ollama"
  },
  "flask": {
    "debug": true
  },
  "database": {
    "connection_pool": {
      "min_connections": 1,
      "max_connections": 5
    },
    "save_queue": {
      "enabled": true,
      "max_size": 50
    },
    "persistence": {
      "save_requests": true,
      "save_songs": true
    }
  },
  "request_handling": {
    "song_limits": {
      "max": 100
    }
  }
}
